{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": { 
        "maxLambdaSize": "15mb" 
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "true",
    "PIP_TARGET": ".vercel/python",
    "PIP_NO_CACHE_DIR": "true"
  }
} 